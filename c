*******************************************************************************/

#include <stdio.h>
#include <stdlib.h>
int main(void)
{
    char name[]="王晟伍";
    int age=28;
    char blood='A';
    float height=180;
    double money=8888888;
    printf("123456789012345678901234567890134567890");
    printf("1234567890\n");
    printf("My name is%s\t I am %d years old this year.\n",name,age);
    printf("And my blood type is %c,\t %f height\n",blood,height);
    printf("I have %E dollors.\n",money);

    return 0;
}

